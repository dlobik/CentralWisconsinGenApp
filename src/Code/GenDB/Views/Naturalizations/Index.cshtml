@model IEnumerable<GenDB.Models.Naturalization>

@{
    ViewBag.Title = "Index";
}


<table class="table">
    <tr>
        <th>
            First Name
            @*@Html.DisplayNameFor(model => model.FirstName)*@
        </th>
        <th>
           Last Name
            @*@Html.DisplayNameFor(model => model.LastName)*@
        </th>
        <th>
            DOB
            @*@Html.DisplayNameFor(model => model.DOB)*@
        </th>
        <th>
            Age
            @*@Html.DisplayNameFor(model => model.Age)*@
        </th>
        <th>
            Date of Record
            @*@Html.DisplayNameFor(model => model.DateOfRecord)*@
        </th>
        <th>
            Location
            @*@Html.DisplayNameFor(model => model.Location)*@
        </th>
        <th>
            Country of Origin
            @*@Html.DisplayNameFor(model => model.CountryOfOrigin)*@
        </th>
        <th>
            Series
            @*@Html.DisplayNameFor(model => model.Series)*@
        </th>
        <th>
            Box
            @*@Html.DisplayNameFor(model => model.Box)*@
        </th>
        <th>
            Folder
            @*@Html.DisplayNameFor(model => model.Folder)*@
        </th>
        <th>
            Volume Number
            @*@Html.DisplayNameFor(model => model.VolNumber)*@
        </th>
        <th>
            Page Cert Number
            @*@Html.DisplayNameFor(model => model.PageCertNumber)*@
        </th>
        <th>
            Date of Entry
            @*@Html.DisplayNameFor(model => model.DateOfEntry)*@
        </th>
        <th>
            Port of Entry
            @*@Html.DisplayNameFor(model => model.PortOfEntry)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DOB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Age)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfRecord)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CountryOfOrigin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Series)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Box)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Folder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VolNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PageCertNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfEntry)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PortOfEntry)
        </td>
        <td>
            @using (Html.BeginForm("Details", "Obits", FormMethod.Get))
            {
                <button type="submit"> Add to Cart</button>
            }
        </td>
    </tr>
}

</table>
