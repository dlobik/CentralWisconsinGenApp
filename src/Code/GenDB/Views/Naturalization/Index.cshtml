@model IEnumerable<GenDB.Models.Naturalization>

@{
    ViewBag.Title = "Index";
}




@foreach (var item in Model)
{
    <table class="table">
        <tr>
            <td>
                <table class="table-striped">
                    <tr>
                        <td>Last Name:</td>
                        <td>@item.LastName</td>
                    </tr>
                    <tr>
                        <td>First Name:</td>
                        <td>@item.FirstName</td>
                    </tr>
                    <tr>
                        <td>Date of Birth:</td>
                        <td>@item.DOB</td>
                    </tr>
                    <tr>
                        <td>Age:</td>
                        <td>@item.Age</td>
                    </tr>
                    <tr>
                        <td>Record Type:</td>
                        <td></td>
                    </tr>
                </table>
            </td>
            <td>
                <table class="table-striped">
                    <tr>
                        <td>Series:</td>
                        <td>@item.Series</td>
                    </tr>
                    <tr>
                        <td>Box:</td>
                        <td>@item.Box</td>
                    </tr>
                    <tr>
                        <td>Folder:</td>
                        <td>@item.Folder</td>
                    </tr>
                    <tr>
                        <td>Volume #:</td>
                        <td>@item.VolNumber</td>
                    </tr>
                    <tr>
                        <td>Page or Certification #:</td>
                        <td>@item.PageCertNumber</td>
                    </tr>
                </table>
            </td>
            <td>
                <table class="table-striped">
                    <tr>
                        <td>Country of Origin:</td>
                        <td>@item.CountryOfOrigin</td>
                    </tr>
                    <tr>
                        <td>Port of Entry:</td>
                        <td>@item.PortOfEntry</td>
                    </tr>
                    <tr>
                        <td>Date of Entry:</td>
                        <td>@item.DateOfEntry</td>
                    </tr>
                    <tr>
                        <td>County Filed:</td>
                        <td>@item.CountyID</td>
                    </tr>
                    <tr>
                        <td>Date Filed:</td>
                        <td>@item.DateOfRecord</td>
                    </tr>
                </table>
            </td>
            <td>
                <table>
                    <tr>
                        <td>
                            @using (Html.BeginForm("Details", "Obituary", FormMethod.Get))
                            {
                                <button type="submit">Add to Cart</button>
                            }
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>

}
